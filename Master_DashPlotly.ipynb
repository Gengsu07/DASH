    "        .groupby(['DATEBAYAR','KET'],as_index=False).sum(),\n",
    "        x='DATEBAYAR',\n",
    "        y='NOMINAL',\n",
    "        # facet_row = 'KET',\n",
    "        hover_data = 'NOMINAL',\n",
    "        # animation_frame='KET',\n",
    "        range_y = [0, 500000000000],\n",
    "        labels = {'NOMINAL':'',\n",
    "                  'DATEBAYAR':''},\n",
    "        title = 'Penerimaan Pajak Bruto 2023'\n",
    "        ).update_layout(\n",
    "        title = dict(x=0.5, y=0.9,\n",
    "                     font=dict(size=24)),\n",
    "        yaxis_visible=False,\n",
    "        plot_bgcolor='white',\n",
